
{
  "mcpServers": {
    "google-maps": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-google-maps"
      ],
      "env": {
        "GOOGLE_MAPS_API_KEY": "xxx"
      }
    },
  
    "line-bot": {
      "command": "node",
      "args": [
        "/xxx/line-bot-mcp-server/dist/index.js"
      ],
      "env": {
        "CHANNEL_ACCESS_TOKEN": "xxx",
        "DESTINATION_USER_ID": "xxx"
      }
    }
  }
}

